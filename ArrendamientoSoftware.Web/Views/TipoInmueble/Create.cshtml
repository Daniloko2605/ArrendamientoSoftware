@model TipoInmuebleDTO

@{
    ViewData["Title"] = "Nuevo Inmueble";
}

<div class="pagetitle">
    <h1>Inmuebles</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Dashboard</a></li>
            <li class="breadcrumb-item"><a asp-action="Index">Inmuebles</a></li>
            <li class="breadcrumb-item active">Nuevo Inmueble</li>
        </ol>
    </nav>
</div>


<section class="section dashboard">


    <div class="card">
        <div class="card-body">
            <h5 class="card-title text-primary">Nuevo Inmueble</h5>

            <form class="row g-3" asp-action="Create" method="POST">
                <div class="col-md-12">
                    <div class="form-floating">
                        <input asp-for="Id" class="form-control" id="floatingName" placeholder="@Html.DisplayFor(model => model.Id)">
                        <label asp-for="Id"></label>
                        <span asp-validation-for="Id" class="text-danger"></span>
                    </div>
                </div>

                <div class="text-center">
                    <button type="submit" class="btn btn-primary">Guardar</button>
                    <a asp-action="Index" class="btn btn-secondary">Volver</a>
                </div>
            </form>

        </div>
    </div>
</section>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}}